#addition
a =6
b =9
print(a+b)

#substraction
m =5
n =6
print(m-n)

#multiplication
x =21
y =11
print(x*y)

#division
l =72
g =12
print (l/g)

#the remainder in the answer
d =7
p =2
print(d%p)

#power of
r =4
s =5
print(r**s)

#if the number is a decimal, the number rounds down instead of up
j =10
k =4
print(j//k)